@using static SnipEx.Common.PopUpMessages

@if (TempData.ContainsKey(ErrorMessage))
{
    <script>
        toastr.error(`@TempData[ErrorMessage]`, "", { timeOut: 5000, closeButton: true });
    </script>
}
 
